{
    "name": "vmlyr-drupal/kit",
    "description": "Project template for VMLY&R Drupal 8 projects with composer.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Garrett Rathbone",
            "role": "Technology Specialist"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/dinbror/blazy/archive/1.8.2.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.3",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "dinbror/blazy": "^1.8",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/address": "^1.8",
        "drupal/admin_toolbar": "^2.2",
        "drupal/advagg": "^4.1",
        "drupal/allowed_formats": "1.x-dev",
        "drupal/better_exposed_filters": "^4.0-dev",
        "drupal/bg_image_formatter": "^1.14",
        "drupal/blazy": "^2.1",
        "drupal/block_class": "^1.2",
        "drupal/block_field": "1.x-dev",
        "drupal/classy_paragraphs": "1.x-dev",
        "drupal/config_export_ignore": "^1.0",
        "drupal/config_ignore": "^2.0-dev",
        "drupal/config_split": "1.x-dev",
        "drupal/config_update": "1.7",
        "drupal/core-recommended": "~9.0.0",
        "drupal/current_page_crumb": "^1.3",
        "drupal/devel": "3.x-dev",
        "drupal/diff": "^1.0",
        "drupal/editor_file": "^1.5",
        "drupal/email_registration": "1.x-dev",
        "drupal/entity_reference_revisions": "^1.8",
        "drupal/environment_indicator": "^3.7",
        "drupal/extlink": "^1.3",
        "drupal/field_group": "^3.1",
        "drupal/focal_point": "^1.4",
        "drupal/google_tag": "^1.4",
        "drupal/imageapi_optimize": "4.x-dev",
        "drupal/imageapi_optimize_resmushit": "1.x-dev",
        "drupal/link_class": "1.x-dev",
        "drupal/linkit": "6.0.0-beta1",
        "drupal/mail_login": "^1.1",
        "drupal/media_responsive_thumbnail": "^1.1",
        "drupal/memcache": "^2.1",
        "drupal/menu_link_attributes": "1.x-dev",
        "drupal/menu_trail_by_path": "^1.3",
        "drupal/metatag": "^1.13",
        "drupal/migrate_plus": "^5.1",
        "drupal/migrate_tools": "^5.0",
        "drupal/module_filter": "3.x-dev",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect": "^1.6",
        "drupal/robotstxt": "^1.4",
        "drupal/schema_metatag": "^1.5",
        "drupal/shield": "^1.4",
        "drupal/simple_sitemap": "^3.6",
        "drupal/smart_trim": "^1.3",
        "drupal/stage_file_proxy": "^1.0",
        "drupal/structure_sync": "1.x-dev",
        "drupal/svg_image": "^1.14",
        "drupal/swiftmailer": "2.x-dev",
        "drupal/telephone_formatter": "^1.0",
        "drupal/telephone_validation": "^2.3",
        "drupal/token": "^1.7",
        "drupal/video_embed_field": "^2.4",
        "drupal/viewsreference": "2.x-dev",
        "drush/drush": "*",
        "topfloor/composer-cleanup-vcs-dirs": "^1.0",
        "vlucas/phpdotenv": "^2.4",
        "vmlyr-drupal/bazo": "^8.0",
        "vmlyr-drupal/biplane": "^8.0",
        "vmlyr-drupal/blackbird": "^8.0",
        "vmlyr-drupal/kit-docksal-commands": "^8.2",
        "webflo/drupal-finder": "^1.2",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "require-dev": {
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.3"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php",
            "scripts/composer/KitScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "drupal-scaffold": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
            "patch -p1 < patches/htaccess.patch"
        ],
        "fix-nfs": "find . -type d > /dev/null",
        "pre-install-cmd": [
            "@fix-nfs",
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "@fix-nfs",
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "@drupal-scaffold"
        ],
        "post-update-cmd": [
            "@drupal-scaffold"
        ]
    },
    "extra": {
        "installer-types": ["library", "drupal-console-language"],
        "installer-paths": {
            ".docksal/commands/kit": ["vmlyr-drupal/kit-docksal-commands"],
            "docroot/core": ["type:drupal-core"],
            "docroot/libraries/{$name}": ["type:drupal-library"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "installer-paths-clean": {
            "KIT Docksal Tools": ".docksal/commands/kit/",
            "Vendor": "vendor/",
            "Drupal Core": "docroot/core",
            "Libraries": "docroot/libraries/*/",
            "Contrib Modules": "docroot/modules/contrib",
            "Contrib Profiles": "docroot/profiles/contrib",
            "Contrib Themes": "docroot/themes/contrib"
        },
        "patches": {
            "drupal/classy_paragraphs": {
                "https://www.drupal.org/node/2830403": "https://www.drupal.org/files/issues/2018-05-12/choose_and_order-2830403-27.patch"
            },
            "drupal/core": {
                "https://www.drupal.org/node/2827921": "https://www.drupal.org/files/issues/2827921-remove-missing-responsive-image-width-exception.patch",
                "https://www.drupal.org/node/2998816": "https://www.drupal.org/files/issues/2018-09-11/arraystringconversion-2998816-12768980.patch",
                "https://www.drupal.org/node/3006729": "https://www.drupal.org/files/issues/2019-05-28/add-context-to-media-iframe-3006729-5.patch"
            },
            "drupal/linkit": {
                "Remove entity labels match escaping": "patches/linkit-label-escape-remove.patch"
            },
            "drupal/simple_gmap": {
                "https://www.drupal.org/node/2863979": "https://www.drupal.org/files/issues/2018-11-21/simple_gmap-address-formatter-2863979-36.patch"
            },
            "drupal/stage_file_proxy": {
                "https://www.drupal.org/node/2882986": "https://www.drupal.org/files/issues/stage_file_proxy-basic-auth-2882986-3-D8.patch"
            },
            "drupal/svg_image": {
                "https://www.drupal.org/node/3023212": "https://www.drupal.org/files/issues/2018-12-31/missing_schema_field_formatter.patch"
            }
        },
        "patchLevel": {
            "drupal/core": "-p2"
        }
    }
}
